post_remove() {
  mv /etc/xdg/autostart/bluetooth-applet.desktop.pacorig /etc/xdg/autostart/bluetooth-applet.desktop
  mv /etc/xdg/autostart/gnome-fallback-mount-helper.desktop.pacorig /etc/xdg/autostart/gnome-fallback-mount-helper.desktop
  mv /etc/xdg/autostart/gnome-sound-applet.desktop.pacorig /etc/xdg/autostart/gnome-sound-applet.desktop
}
